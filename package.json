{
  "name": "starlist-app",
  "version": "1.0.0",
  "description": "Flutter Web with BrowserSync for instant reload",
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true,
  "scripts": {
    "preinstall": "node -e \"const v=Number(process.versions.node.split('.')[0]); if(Number.isNaN(v)||v<20){console.error('Node>=20 required (use `nvm use 20`).'); process.exit(1)}\"",
    "start:bs": "browser-sync start --config bs-config.js",
    "dev": "next dev -p 8081",
    "dev:3002": "next dev -p 3002 -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "report": "bash scripts/progress-report.sh",
    "lint:md": "node scripts/ensure-node20.js && NODE_OPTIONS=--require=./scripts/polyfill-file.js markdown-link-check -c .mlc.json -q **/*.md",
    "lint:markdownlint": "markdownlint-cli --config ./.markdownlint.json --ignore node_modules --ignore .next --ignore build --ignore dist --ignore out --ignore artifact_4544009766_dir --ignore docs/ops/MD_CI_REPORT.md \"**/*.md\"",
    "lint:md:starlist": "python3 scripts/starlist_md_validator.py",
    "lint:md:local": "node scripts/docs/link-check.mjs",
    "check:mail:dns": "node scripts/check_mail_dns.mjs starlist.jp",
    "pricing:final": "bash PRICING_FINAL_SHORTCUT.sh",
    "docs:update-dates": "node scripts/docs/update-last-updated.mjs",
    "docs:diff-log": "node scripts/docs/generate-diff-log.mjs",
    "docs:preflight": "npm run docs:update-dates && npm run lint:md:local && npm run docs:diff-log",
    "linear:create-issue": "ts-node scripts/linear/create_linear_issue.ts",
    "linear:complete-issue": "ts-node scripts/linear/complete_linear_issue.ts",
    "security:runner": "bash scripts/extended_security_runner.sh",
    "security:install:brew": "brew install gitleaks trivy || true",
    "security:install:pipx": "pipx install semgrep || pip install semgrep || true",
    "security:ci": "npm run -s security:runner",
    "security:local": "npm run -s security:install:brew && npm run -s security:install:pipx && npm run -s security:runner",
    "audit:npm": "npm audit --audit-level=high || true",
    "audit:pnpm:jsr": "pnpm audit --registry=https://registry.npmjs.org || true",
    "phase4:observer": "ts-node scripts/phase4/observer/index.ts",
    "phase4:collect": "ts-node scripts/phase4/collector/collect.ts",
    "phase4:kpi": "ts-node scripts/phase4/kpi/aggregate.ts",
    "phase4:sha": "ts-node scripts/phase4/sha-compare.ts",
    "phase4:sweep": "ts-node scripts/phase4/sweep/scan.ts",
    "test:openai": "ts-node src/openai-test.ts",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.5.3",
    "@types/react": "19.2.4",
    "autoprefixer": "^10.4.0",
    "browser-sync": "^2.29.3",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.5.0",
    "markdown-link-check": "^3.14.1",
    "markdownlint-cli": "^0.34.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^5.13.0",
    "@google-cloud/storage": "^7.17.3",
    "@hookform/resolvers": "^3.10.0",
    "@linear/sdk": "^64.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^11.18.2",
    "groq-sdk": "^0.3.3",
    "lucide-react": "^0.344.0",
    "next": "^13.5.0",
    "openai": "^6.9.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.68.0",
    "recharts": "^3.5.1",
    "zod": "^3.25.76"
  }
}
